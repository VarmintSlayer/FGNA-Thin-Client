<?xml version='1.0' encoding='utf-8'?>
<widget id="io.SutterCorps.FGNAHistoricHomesTour" version="0.0.2" android-versionCode="20" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>FGNA Historic Homes Tour</name>
  <description>
      FGNA Historic Homes Tour
  </description>
  <author email="suttercorps@gmail.com" href="http://www.nra.com">
      Sutter Corps
  </author>

  <icon src="res/fgna.png" />
  <content src="index.html" />

  <preference name='phonegap-version' value='cli-8.0.0' />
  <preference name="android-minSdkVersion" value="22" />
  <preference name="android-targetSdkVersion" value="28" />

  <plugin name="cordova-plugin-whitelist" spec="1" />
  <access origin="*" />
  <allow-intent href="http://*/*" />
  <allow-intent href="https://*/*" />
  <allow-intent href="tel:*" />
  <allow-intent href="sms:*" />
  <allow-intent href="mailto:*" />
  <allow-intent href="geo:*" />
  <platform name="android">
      <allow-intent href="market:*" />
  </platform>
	
</widget>
